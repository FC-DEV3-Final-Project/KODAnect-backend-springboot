<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks=".*" files=".*egovframework/.*"/>
    <suppress checks=".*" files=".*application.*\.properties"/>
    <suppress checks="MagicNumberCheck" files=".*domain/.*?/entity/.*"/>
    <suppress checks="MagicNumberCheck" files=".*domain/.*/dto/.*"/>
    <suppress checks="MagicNumberCheck" files=".*domain\.*?\entity\.*"/>
    <suppress checks="MagicNumberCheck" files=".*domain\.*\dto\.*"/>
</suppressions>

